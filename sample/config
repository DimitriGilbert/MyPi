MyPi_mkdir=("$HOME/bin" "$HOME/workspace" "/media/data")
MyPi_apt_pkg=(zsh git htop ntfs-3g nfs-common nfs-kernel-server xserver-xorg mate-desktop-environment-core lightdm celluloid vlc firefox-esr)
MyPi_git_repo=("https://github.com/zsh-users/zsh-autosuggestions" "https://github.com/zsh-users/zsh-syntax-highlighting.git")
MyPi_git_clone_dir=("${ZSH_CUSTOM:-$HOME/.oh-my-zsh/custom}/plugins/zsh-autosuggestions" "${ZSH_CUSTOM:-$HOME/.oh-my-zsh/custom}/plugins/zsh-syntax-highlighting")
MyPi_fstab=("PARTUUID=myextdrive /media/data     ntfs-3g    nofail,uid=1000,gid=1000  0       0" "PARTUUID=myextdrive2  /media/data2     ntfs-3g    nofail,uid=1000,gid=1000  0       0")
MyPi_nfs_export=("/media/data 192.168.127.2(rw,root_squash)" "/media/data 192.168.127.3(rw,root_squash)")
MyPi_docker_cmd=()
MyPi_docker_images=()
MyPi_post_install=()
MyPi_omz_plugin=(docker "docker-compose" sudo common-aliases "zsh-autosuggestion" "zsh-syntax-highlighting")